{
  "name": "Refgen",
  "version": "1.3.0",
  "manifest_version": 2,
  "description": "Generate referrer for current page.",
  "author": "Reorx <novoreorx@gmail.com>",
  "icons": {
    "16": "logo-947.png",
    "48": "logo-947.png",
    "128": "logo-947.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}
